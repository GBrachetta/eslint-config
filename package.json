{
  "name": "@gbrachetta/eslint-config",
  "version": "1.1.7",
  "description": "ESLint configuration for React-based projects based on airbnb and Prettier configs",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write **/*.js",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [
    "javascript",
    "eslint",
    "eslintconfig",
    "eslint-config",
    "airbnb-config",
    "prettier-config",
    "react",
    "hooks",
    "react-hooks",
    "gbrachetta",
    "guillermo brachetta"
  ],
  "author": "Guillermo Brachetta",
  "license": "MIT",
  "prettier": "@gbrachetta/prettier-config",
  "dependencies": {
    "@gbrachetta/prettier-config": "^1.0.0",
    "eslint": "^7.24.0",
    "prettier": "^2.2.1"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "peerDependencies": {
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gbrachetta/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/gbrachetta/eslint-config/issues"
  },
  "homepage": "https://github.com/gbrachetta/eslint-config#readme"
}
